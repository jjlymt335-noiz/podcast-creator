{
  "rewrites": [
    { "source": "/api/v2/:path*", "destination": "/api/noiz-proxy?prefix=v2&apiPath=:path*" },
    { "source": "/api/v1/:path*", "destination": "/api/noiz-proxy?prefix=v1&apiPath=:path*" },
    { "source": "/api/audiox/:path*", "destination": "/api/noiz-proxy?prefix=audiox&apiPath=:path*" },
    { "source": "/((?!api).*)", "destination": "/index.html" }
  ]
}
